/**app.wxss**/
@font-face {
	font-family: 'iconfont';
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: normal;
	font-style: normal;
}


.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-icon:before { content: "\e60d"; }

.icon-lianxi:before { content: "\e77e"; }

.icon-shouye:before { content: "\e680"; }

.icon-chanpin:before { content: "\e600"; }

.icon-liuyan:before { content: "\e638"; }
page{
  background: #E6E6E6;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0 0;
  box-sizing: border-box;
} 
.banner-swiper{
  display: block;
  width: 100%;
  height: 190px;
}
.banner-swiper image{
  display: block;
  width: 100%;
  height: 190px;
}
.subbanner{
  display: block;
  width: 100%;
  height: 190px;
}
.subbanner image{
  display: block;
  width: 100%;
  height: 190px;
}
.bottom{
  padding: 10px 0;
  font-size: 24rpx;
  color: #666;
  line-height: 46rpx;
  text-align: center;
}
.nodata{
  padding: 5px 0;
  font-size: 26rpx;
  color: #666;
  line-height: 30rpx;
  text-align: center;
}
.footer{
  width: 100%;
  height: 48px;
  border-top: 1px solid #666;
  position: fixed;
  left: 0;
  bottom: 0;
  background: #fff;
}
.footer navigator{
  display: inline-block;
  width: 20%;
  height: 48px;
  font-size: 12px;
  line-height: 16px;
  color: #9f9f9f;
  text-align: center;
  box-sizing: border-box;
}
.footer navigator view{
  font-size: 20px;
  display: block;
  line-height: 20px;
  padding: 6px 0 2px 0;
}
.footer navigator.current{
  color:#2199EE;
}
.footer_h{
  height: 50px;
}
.topnav{
  width: 100%;
  text-align: left;
  background: #fff;
  font-size: 0;
}
.topnav navigator{
  float:left;
  height: 28px;
  border-bottom: 2px solid #fff;
  padding: 0 2px;
  margin: 0 5px;
  font-size: 12px;
  color: #666;
  line-height: 28px;
}
.topnav navigator.current{
  border-bottom: 2px solid #2199EE;
  color: #2199EE;
}